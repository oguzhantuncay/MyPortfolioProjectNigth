@model MyPortfolioProjectNigth.Models.About
@{
    ViewBag.Title = "UpdateGetHakkında";
    Layout = "~/Views/Admin/Index.cshtml";
}


<form method="post">
    @Html.HiddenFor(x => x.AboutId)

    <div class="form-group">
        <label>Deneyim Başlığı</label>
        @Html.TextBoxFor(x => x.Title, new { @class = "form-control" })
    </div>

    <div class="form-group">
        <label>Deneyim Açıklaması</label>
        @Html.TextBoxFor(x => x.Descraption, new { @class = "form-control" })
    </div>

    <button type="submit" class="btn btn-info">Güncelle</button>
</form>